#include <stdio.h>

int main() 
{
    int age = -1;
    
    printf("Please enter your age:");
    scanf("%d", &age);
    
    if (age <= 18){
        int try = 0;
        for (try = 0 ;try <= 2 ;try++);
    printf("your age is %d which is too young\n", age);
    }else{
        int try = 0;
        for (try = 0 ;try <= 1 ;try++){
        printf("your too old\n");
        printf("please try again:");
        scanf("%d", &age);
        }
    }
    return 0;
}
